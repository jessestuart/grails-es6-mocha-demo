{
  "presets": ["es2015"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "development": {
      "plugins": [
        "transform-es2015-modules-amd"
      ],
      "moduleIds": true,
      "sourceRoot": ".*\/grails-app\/assets\/javascripts",
      "moduleRoot": null
    }
  }
}
